.goods-details-content {
  padding-bottom: 50px;
  background-color: #eee;
}
.goods-details-content .popup-content {
  box-sizing: border-box;
  width: 100vw;
  height: 50vh;
  padding: 30rpx;
  border-top-right-radius: 15px;
  border-top-left-radius: 15px;
  background-color: #fff;
}
.goods-details-content .popup-content .popup-content-img-text {
  padding-bottom: 20rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #ddd;
}
.goods-details-content .popup-content .popup-content-img-text .popup-img {
  width: 150rpx;
  height: 150rpx;
  border-radius: 15px;
  display: block;
  margin-right: 20rpx;
}
.goods-details-content .popup-content .popup-content-img-text .popup-text .goods-price {
  font-size: 30rpx;
  color: #c00;
}
.goods-details-content .popup-content .popup-content-img-text .popup-text .goods-count {
  font-size: 12px;
  color: #aaa;
}
.goods-details-content .popup-content .buy-count {
  padding: 30rpx;
  display: flex;
  align-items: center;
}
.goods-details-content .popup-content .buy-count .count {
  text-align: center;
  width: 100rpx;
}
.goods-details-content .popup-content .buy-count .minus {
  margin-left: 10rpx;
  width: 60rpx;
  height: 60rpx;
  background-image: url(data:image/png;base64,);
  background-position: center center;
  background-repeat: no-repeat;
  background-size: cover;
}
.goods-details-content .popup-content .buy-count .add {
  width: 60rpx;
  height: 60rpx;
  background-image: url(data:image/png;base64,);
  background-position: center center;
  background-repeat: no-repeat;
  background-size: cover;
}
.goods-details-content .popup-content .popup-bottom {
  box-sizing: border-box;
  position: fixed;
  bottom: 0;
  left: 0;
  height: 7vh;
  width: 100vw;
  display: flex;
  align-items: center;
  justify-content: space-around;
  padding: 0 20rpx;
}
.goods-details-content .popup-content .popup-bottom .add-cart {
  text-align: center;
  color: #fff;
  width: 100%;
  padding: 20rpx 0;
  border-radius: 999px;
  background-color: #ffa91a;
}
.goods-details-content .popup-content .popup-bottom .buy {
  text-align: center;
  color: #fff;
  width: 100%;
  padding: 20rpx 0;
  border-radius: 999px;
  background-color: #f01626;
}
.goods-details-content .goodsdetai-img {
  width: 750rpx;
  height: 50vh;
}
.goods-details-content .goodsdetai-img .img {
  width: 100%;
  height: 100%;
}
.goods-details-content .goodsdetai-img .img image {
  width: 100%;
  height: 100%;
}
.goods-details-content .p-title {
  width: 95%;
  margin: 20rpx auto;
  border-radius: 10rpx;
  background-color: #fff;
}
.goods-details-content .p-title .title-text {
  padding: 20rpx;
  margin-bottom: 20rpx;
}
.goods-details-content .p-title .price-text {
  padding: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.goods-details-content .p-title .price-text .price {
  font-size: 50rpx;
  color: #c00;
}
.goods-details-content .p-title .price-text .collect {
  display: flex;
  align-items: center;
}
.goods-details-content .p-title .price-text .collect .collect-icon {
  width: 45rpx;
  height: 45rpx;
  margin-right: 10rpx;
  background-image: url(data:image/png;base64,);
  background-position: center center;
  background-repeat: no-repeat;
  background-size: cover;
}
.goods-details-content .p-title .price-text .collect .collect-icon.collect {
  background-image: url(data:image/png;base64,);
}
.goods-details-content .image-text {
  padding: 5%;
  background-color: #fff;
  width: 100vw;
}
.goods-details-content .image-text .details-text {
  display: block;
  color: #c00;
  margin-bottom: 15rpx;
  font-weight: 50rpx;
  font-weight: bold;
  content: '';
}
.goods-details-content .image-text .imag-content {
  width: 100%;
}
.goods-details-content .image-text .imag-content image {
  width: 90%;
}
.goods-details-content .specifications {
  margin-top: 20rpx;
  padding: 5%;
  background-color: #fff;
  width: 100vw;
}
.goods-details-content .specifications .specifications-content {
  display: flex;
  align-items: center;
  font-size: 25rpx;
  line-height: 50rpx;
}
.goods-details-content .specifications .specifications-content .attr-name {
  width: 30vw;
}
.goods-details-content .specifications .specifications-content .attr-value {
  margin-left: 20rpx;
  width: 42vw;
}

