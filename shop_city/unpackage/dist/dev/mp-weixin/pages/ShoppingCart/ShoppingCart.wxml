<view class="shopping-cart"><block wx:if="{{!productList}}"><view class="no-shopping"><van-empty vue-id="62e064c0-1" description="购物车为空" data-com-type="wx" bind:__l="__l"></van-empty></view></block><block wx:if="{{productList}}"><view class="shoppingCartList"><checkbox-group><block wx:for="{{cartList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view data-ref="{{item.id}}" class="shoppingCartItem vue-ref-in-for"><label><checkbox class="round red" checked="{{item.isChecked}}" data-event-opts="{{[['tap',[['togglecart',[['o',['id',item.id]]]]]]]}}" bindtap="__e"></checkbox></label><van-card class="card-item" vue-id="{{'62e064c0-2-'+index}}" price="{{item.price}}" desc="{{item.name}}" thumb="{{item.src}}" data-com-type="wx" bind:__l="__l" vue-slots="{{['desc','footer']}}"><view class="van-multi-ellipsis--l2" slot="desc">{{item.name}}</view><view class="number" slot="footer"><view data-event-opts="{{[['tap',[['add',['$0'],[[['cartList','id',item.id,'id']]]]]]]}}" class="add" bindtap="__e">+</view>{{''+item.num+''}}<view data-event-opts="{{[['tap',[['reduce',['$0'],[[['cartList','id',item.id,'id']]]]]]]}}" class="reduce" bindtap="__e">-</view></view></van-card></view></block></checkbox-group><van-toast vue-id="62e064c0-3" id="van-toast" data-com-type="wx" bind:__l="__l"></van-toast><van-submit-bar vue-id="62e064c0-4" price="{{cartTotalPrice}}" button-text="提交订单" data-event-opts="{{[['^submit',[['placeOrder']]]]}}" data-com-type="wx" bind:submit="__e" bind:__l="__l"></van-submit-bar></view></block></view>